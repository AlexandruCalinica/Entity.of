{
  "manifest_version": 2,
  "name": "Entity.of Dev Tools",
  "version": "1.0.0",
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],

  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "48": "icon.svg"
    },
    "default_title": "Entity.of Dev Tools",
    "default_popup": "popup.html"
  },

  "permisions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus",
    "webRequest",
    "webRequestBlocking"
  ]
}